  <PropertyGroup>
    <AreasRoot>$(SolutionDir)\Web.TypeScript\Areas</AreasRoot>
    <PackagesOutput>.\.nuget\packages\</PackagesOutput>
    <PackagesTemp>.\.nuget\packages.temp</PackagesTemp>
    <NuSpec>Web.TypeScript.nuspec</NuSpec>
    <TempOutput>.\.nuget\packages.temp\Web.TypeScript</TempOutput>
  </PropertyGroup>
  <Target Name="StagePackage" Inputs="@(TypeScriptAreas)" Outputs="./out/packages/%(TypeScriptAreas.Filename).nupkg">
    <PropertyGroup>
      <AreaName>%(TypeScriptAreas.Filename)</AreaName>
      <BasePackageOutput>$(TempOutput)\content\Areas\$(AreaName)</BasePackageOutput>
    </PropertyGroup>
    <ItemGroup>
      <TypeScriptFiles Include="$(AreasRoot)\$(AreaName)\**\*.js" Exclude="$(AreasRoot)\$(AreaName)\**\*.Test.js;$(AreasRoot)\$(AreaName)\**\*.Sample.js" />
      <CssFiles Include="$(AreasRoot)\$(AreaName)\Styles\**\*.css" />
    </ItemGroup>
    <MakeDir Directories="$(PackagesOutput)" />
    <MakeDir Directories="$(TempOutput)" />
    <MakeDir Directories="$(TempOutput)\content" />
    <!-- Shared components -->
    <Copy SourceFiles="@(TypeScriptFiles)" DestinationFolder="$(BasePackageOutput)\%(TypeScriptFiles.RecursiveDir)" Condition="$(AreaName) == 'Shared' " />
    <Copy SourceFiles="@(CssFiles)" DestinationFolder="$(BasePackageOutput)\Styles\%(CssFiles.RecursiveDir)" Condition="$(AreaName) == 'Shared' " />
    <!-- Area-specific components -->
    <Copy SourceFiles="@(TypeScriptFiles)" DestinationFolder="$(BasePackageOutput)\Content\js\%(TypeScriptFiles.RecursiveDir)" Condition="$(AreaName) != 'Shared' " />
    <Copy SourceFiles="@(CssFiles)" DestinationFolder="$(BasePackageOutput)\Content\css\%(CssFiles.RecursiveDir)" Condition="$(AreaName) != 'Shared' " />
  </Target>
  <Target Name="PackPackage">
    <CallTarget Targets="GetVersion" />
    <Copy SourceFiles=".\$(NuSpec)" DestinationFolder="$(TempOutput)" />
    <Exec WorkingDirectory="$(TempOutput)" Command="nuget pack .\$(NuSpec) -OutputDirectory ..\..\..\$(PackagesOutput) -version $(GitVersion_NuGetVersion)" />
  </Target>
  <Target Name="CleanPackageStage">
    <RemoveDir Directories="$(PackagesTemp)" />
  </Target>
